{
  "manifest_version": 3,
  "name": "hellointerview cleaner",
  "version": "1.0",
  "description": "Clean up page elements with one click",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "https://www.hellointerview.com/*"
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Clean Page",
    "default_icon": "logo.png"
  }
}